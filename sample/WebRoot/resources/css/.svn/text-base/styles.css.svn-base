.x-table-layout { /* fix for form elements with no border clipping */
    width: 100%;
}
.x-table-form-layout-ct .x-table-layout-cell .x-form-field { /* fix for form elements with no border clipping */
    margin: 0px;
}
.x-table-form-layout-ct .x-table-layout-cell .x-form-trigger { /* ie fix for trigger field positioning */
    margin-top: 0px;
}
